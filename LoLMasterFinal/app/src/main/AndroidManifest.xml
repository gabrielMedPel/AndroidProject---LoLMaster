<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gabriel.medpel.lolmasterfinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/challenger"
        android:label="LoL Master"
        android:roundIcon="@drawable/challenger"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoLMasterFinal"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.login.LoginActivity"></activity>
        <activity android:name=".SummonerActivity" />
        <activity android:name=".MainScreenActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoLPediaActivity"
            android:label="@string/app_name" />
    </application>

</manifest>